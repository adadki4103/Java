public class car
{
  private String carMake, carModel, antAns;
  private int carYear;
  private final int antiqueYear = 1969;
  
// Sets up car details
  public car(String cmake, String cmodel, String aans, int cyear, int antyear)
  {
    carMake = cmake;
    carModel = cmodel;
    antAns = aans;
    carYear = cyear;
    antiqueYear = antyear;
  }
  
// Is it antique? Antique year is 1969.
public boolean isAntiq2ue
{
 return if(carYear <= antiqueYear)
 {
   antAns = "Yes";
 }
 else
 {
   antAns = "No";
 }
}
  
