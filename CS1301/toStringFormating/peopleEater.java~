public class peopleEater
{
  private String fname;
  private String lname; 
  private int zip;
  
  public peopleEater(String first, String last, int zipCode);
  {
    fname = first;
    lname = last;
    zip = zipCode;
  }
  
  public String toString()
  {
    return fname + "\t" + lname + "\t" + zip;
  }
  